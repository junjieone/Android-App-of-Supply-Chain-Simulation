<ScrollView xmlns:Android="http://schemas.android.com/apk/res/android"
    Android:layout_height="fill_parent"
    Android:layout_width="fill_parent"
    Android:background="#ffffffff"
    Android:scrollbars="vertical"
    Android:clickable="true">


    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="match_parent">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="115dp"
            android:id="@+id/imageView3"
            android:layout_row="0"
            android:layout_column="0"
            android:src="@drawable/logo"
            android:background="#ff3995ff" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Location:"
            android:id="@+id/textView1"
            android:layout_row="1"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="20dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/textView_location"
            android:layout_row="2"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="18dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Basic Parameter:"
            android:id="@+id/textView3"
            android:layout_row="3"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="20dp"
            android:layout_marginTop="5dp" />
        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_row="4"
            android:layout_column="0">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Rounds Number:"
                android:id="@+id/textView4"
                android:layout_row="0"
                android:layout_column="0"
                android:textColor="#ff3995ff"
                android:textIsSelectable="false"
                android:layout_marginLeft="8dp"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_marginTop="5dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Chain Number:"
                android:id="@+id/textView5"
                android:layout_row="1"
                android:layout_column="0"
                android:textColor="#ff3995ff"
                android:textIsSelectable="false"
                android:layout_marginLeft="8dp"
                android:textSize="18dp"
                android:textStyle="bold" />
            <EditText
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/editText_multiRounds"
                android:layout_row="0"
                android:layout_column="1"
                android:background="@drawable/text_round"
                android:layout_marginLeft="15dp"
                android:textColor="#ff3995ff"
                android:textStyle="bold" />
            <EditText
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/editText_multiChains"
                android:layout_row="1"
                android:layout_column="1"
                android:background="@drawable/text_round"
                android:layout_marginLeft="15dp"
                android:textColor="#ff3995ff"
                android:textStyle="bold" />
        </GridLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Demand Parameter:"
            android:id="@+id/textView3"
            android:layout_row="5"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="20dp"
            android:layout_marginTop="5dp" />
    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_row="6"
        android:layout_column="0">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Mean/Parameter A:"
            android:id="@+id/textView4"
            android:layout_row="0"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:layout_marginLeft="8dp"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Variance/Parameter B:"
            android:id="@+id/textView5"
            android:layout_row="1"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:layout_marginLeft="8dp"
            android:textSize="18dp"
            android:textStyle="bold" />
        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/editText_multiMean"
            android:layout_row="0"
            android:layout_column="1"
            android:background="@drawable/text_round"
            android:layout_marginLeft="15dp"
            android:textColor="#ff3995ff"
            android:textStyle="bold" />
        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/editText_multiVar"
            android:layout_row="1"
            android:layout_column="1"
            android:background="@drawable/text_round"
            android:layout_marginLeft="15dp"
            android:textColor="#ff3995ff"
            android:textStyle="bold" />
        </GridLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Character Parameter:"
            android:id="@+id/textView3"
            android:layout_row="7"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="20dp"
            android:layout_marginTop="5dp" />
        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_row="8"
            android:layout_column="0">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="unit cost of overstock:"
                android:id="@+id/textView4"
                android:layout_row="0"
                android:layout_column="0"
                android:textColor="#ff3995ff"
                android:textIsSelectable="false"
                android:layout_marginLeft="8dp"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_marginTop="5dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="unit cost of shortage:"
                android:id="@+id/textView5"
                android:layout_row="1"
                android:layout_column="0"
                android:textColor="#ff3995ff"
                android:textIsSelectable="false"
                android:layout_marginLeft="8dp"
                android:textSize="18dp"
                android:textStyle="bold" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="unit cost of goods:"
                android:id="@+id/textView5"
                android:layout_row="2"
                android:layout_column="0"
                android:textColor="#ff3995ff"
                android:textIsSelectable="false"
                android:layout_marginLeft="8dp"
                android:textSize="18dp"
                android:textStyle="bold" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="unit price of goods:"
                android:id="@+id/textView5"
                android:layout_row="3"
                android:layout_column="0"
                android:textColor="#ff3995ff"
                android:textIsSelectable="false"
                android:layout_marginLeft="8dp"
                android:textSize="18dp"
                android:textStyle="bold" />
            <EditText
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/editText_multiOverstock"
                android:layout_row="0"
                android:layout_column="1"
                android:background="@drawable/text_round"
                android:layout_marginLeft="15dp"
                android:textColor="#ff3995ff"
                android:textStyle="bold" />
            <EditText
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/editText_multiShortage"
                android:layout_row="1"
                android:layout_column="1"
                android:background="@drawable/text_round"
                android:layout_marginLeft="15dp"
                android:textColor="#ff3995ff"
                android:textStyle="bold" />
            <EditText
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/editText_multiCost"
                android:layout_row="2"
                android:layout_column="1"
                android:background="@drawable/text_round"
                android:layout_marginLeft="15dp"
                android:textColor="#ff3995ff"
                android:textStyle="bold" />
            <EditText
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/editText_multiPrice"
                android:layout_row="3"
                android:layout_column="1"
                android:background="@drawable/text_round"
                android:layout_marginLeft="15dp"
                android:textColor="#ff3995ff"
                android:textStyle="bold" />

        </GridLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="History Results:"
            android:id="@+id/textView6"
            android:layout_row="9"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="20dp"
            android:layout_marginTop="5dp" />
        <ListView
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:id="@+id/listView"
            android:layout_row="10"
            android:layout_column="0"
            android:layout_marginTop="10dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Decision of this Round:"
            android:id="@+id/textView6"
            android:layout_row="11"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal" />


        <EditText
            android:layout_width="180dp"
            android:layout_height="21dp"
            android:id="@+id/editText_multiDecision"
            android:layout_row="12"
            android:layout_column="0"
            android:background="@drawable/text_round_blue"
            android:layout_marginTop="5dp"
            android:layout_gravity="center_horizontal" />
        <Button
            android:layout_width="265dp"
            android:layout_height="48dp"
            android:background="@drawable/button"
            android:text="Confirm"
            android:id="@+id/button_multiConfirm"
            android:layout_row="13"
            android:layout_column="0"
            android:clickable="true"
            android:textColor="#ffffffff"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Rounds:"
            android:id="@+id/textView6"
            android:layout_row="14"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_marginTop="5dp"
            android:layout_gravity="center_horizontal" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/textView_round"
            android:layout_row="15"
            android:layout_column="0"
            android:textColor="#ff3995ff"
            android:textIsSelectable="false"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            android:textSize="18dp"
            android:layout_gravity="center_horizontal" />
        <Button
            android:layout_width="265dp"
            android:layout_height="48dp"
            android:background="@drawable/button"
            android:text="Next Round"
            android:id="@+id/button_multiRounds"
            android:layout_row="16"
            android:layout_column="0"
            android:clickable="true"
            android:textColor="#ffffffff"
            android:textStyle="bold"
            android:layout_marginTop="5dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="5dp" />


</GridLayout>
</ScrollView>